<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <% sitemap = hb_sitemap.select { |r| r.data.exactmatch } %>
  <% if sitemap.count > 0 %>
    <% sitemap.each do |r| %>
      <% r.data.exactmatch.each do |m| %>
  <key><%= m %></key>
  <string><%= r.data.anchor || r.page_name  %></string>
      <% end %>
    <% end %>
  <% end %>
</dict>
</plist>
